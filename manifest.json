{
  "name": "Tab Saver",
  "description": "Save all tabs in the active window so that they can be opened later",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "Saved tabs",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
